    }
  },
  "node_sets_file": "$BASE_DIR/node_sets.json",
  "networks": {
    "nodes": [
      {
        "nodes_file": "$NETWORK_NODES_DIR/S1nonbarrel_neurons/nodes.h5",
        "populations": {
           "S1nonbarrel_neurons": {
              "alternate_morphologies": {
