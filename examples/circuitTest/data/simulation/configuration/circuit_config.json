        "populations": {
           "VPM": {
              "type": "virtual"
           }
        }
      },
      {
        "nodes_file": "$NETWORK_NODES_DIR/POm/nodes.h5",
        "populations": {
           "POm": {
              "type": "virtual"
           }
        }
      }
    ],
    "edges": [
      {
        "edges_file": "$NETWORK_EDGES_DIR/edges.h5",
        "populations": {
           "S1nonbarrel_neurons__S1nonbarrel_neurons__chemical": {
              "type": "chemical"
